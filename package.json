{
  "name": "react-hook-form-material-ui",
  "main": "index.js",
  "version": "0.13.1",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 9009 -s public",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "test": "react-scripts test",
    "build-storybook": "build-storybook -s public",
    "deploy": "npm run build-storybook && cd storybook-static && now --name react-hook-form-material-ui --prod",
    "deploy-beta": "npm run build-storybook && cd storybook-static && now --name react-hook-form-material-ui-beta --prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "sort-imports": "off",
      "import/first": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "/dist"
  ],
  "peerDependencies": {
    "@material-ui/core": "> 4.7 <5",
    "@material-ui/pickers": "3.2.x",
    "react-hook-form": "4.10.1",
    "@date-io/date-fns": "1.3.x",
    "date-fns": "2.7.x"
  },
  "devDependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.14",
    "@storybook/source-loader": "^5.3.14",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "date-fns": "2.9.0",
    "mutation-observer": "^1.0.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "4.10.1",
    "react-scripts": "^3.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
